{
  "host": {
    "type": "string",
    "default": "127.0.0.1",
    "title": "服务Host"
  },
  "port": {
    "type": "number",
    "default": 8001,
    "title": "HTTP代理端口"
  },
  "socksPort": {
    "type": "number",
    "default": 8002,
    "title": "Socks代理端口"
  },
  "webPort": {
    "type": "number",
    "default": 8003,
    "title": "Web服务端口"
  },
  "mode": {
    "type": "string",
    "default": "VPN",
    "title": "代理模式",
    "options": [{
        "value": "VPN",
        "title": "VPN"
      },
      {
        "value": "proxy",
        "title": "Proxy"
      }
    ]
  },
  "vpnMode": {
    "type": "number",
    "default": 0,
    "title": "VPN模式",
    "options": [{
        "value": 0,
        "title": "全局模式"
      },
      {
        "value": 1,
        "title": "白名单"
      },
      {
        "value": 2,
        "title": "黑名单"
      }
    ]
  },
  "tunnelType": {
    "type": "string",
    "default": "vpn2http",
    "title": "VPNTunnel模式",
    "options": [{
        "value": "vpn2socks",
        "title": "vpn2socks"
      },
      {
        "value": "vpn2http",
        "title": "vpn2http"
      }
    ]
  },
  "script": {
    "type": "boolean",
    "default": false,
    "title": "开启脚本功能"
  },
  "hideConnect": {
    "type": "boolean",
    "default": true,
    "title": "隐藏Connect记录"
  },
  "forceProxyHttps": {
    "type": "boolean",
    "default": false,
    "title": "强制抓包HTTPS"
  },
  "dangerouslyIgnoreUnauthorized": {
    "type": "boolean",
    "default": true,
    "title": "忽略证书错误"
  },
  "wsIntercept": {
    "type": "boolean",
    "default": true,
    "title": "开启websocket代理"
  },
  "silent": {
    "type": "boolean",
    "default": true,
    "title": "不输出日志"
  }
}